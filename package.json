        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "^5.0.1",
        "react-zoom-pan-pinch": "^2.1.3",
        "typescript": "^4.7.4"
